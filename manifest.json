{
   "action": {
      "default_icon": {
         "128": "icon.png"
      }
   },
   "content_scripts": [
      {
         "js": [
            "content.js"
         ],
         "matches": [
            "https://*.youtube.com/*"
         ]
      }
   ],
   "description": "Youtube-Ad-blocker-Reminder-Remover",
   "icons": {
      "128": "icon.png"
   },
   "manifest_version": 3,
   "name": "Youtube-Ad-blocker-Reminder-Remover",
   "permissions": [
      "scripting"
   ],
   "version": "1.0"
}